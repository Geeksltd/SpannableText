<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Zebble.SpannableTextView</id>
    <version>1.0.103</version>
    <title>SpannableTextView for Zebble (Xamarin)</title>
    <projectUrl>https://github.com/Geeksltd/Zebble.SpannableTextView</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/Geeksltd/Zebble.SpannableTextView/master/Shared/NuGet/Icon.png</iconUrl>
    <copyright>Copyright ©2018 Geeks Ltd - All rights reserved.</copyright>
    <description>SpannableTextView is an extended Zebble TextView that is able to render simple markups such color and bold tags.</description>
    <authors>Geeks Ltd</authors>
    <owners>Geeks Ltd</owners>
    <dependencies>
      <group targetFramework="MonoAndroid10">
        <dependency id="HtmlAgilityPack" version="1.6.12" />
      </group>
      <group targetFramework="Xamarin.iOS10">
        <dependency id="HtmlAgilityPack" version="1.6.12" />
      </group>
      <group targetFramework="UAP10">
        <dependency id="HtmlAgilityPack" version="1.6.12" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\lib\Zebble.SpannableTextView.iOS.*" target="lib\Xamarin.iOS10\" />
    <file src="..\..\lib\Zebble.SpannableTextView.Android.*" target="lib\MonoAndroid10\" />
    <file src="..\..\lib\Zebble.SpannableTextView.UWP.*" target="lib\UAP10\" />
  </files>
</package>